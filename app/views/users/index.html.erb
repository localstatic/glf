<h2>Users</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th></th>
      <th>Login Count</th>
      <th>Last Login</th>
      <th></th>
    </tr>
  </thead>
  <tfoot>
    <tr>
      <td colspan="2">
      </td>
    </tr>
  </tfoot>
  <tbody>
    <%= render @users %>
  </tbody>
</table>

<%= will_paginate @users %>
<% if can? :create, User %>
  <%= link_to "Add User", new_user_path %>
<% end %>
