<%= form_for @place do |f| %>
  <div class="controlgroup">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="controlgroup">
    <%= f.label :url %>
    <%= f.text_field :url %>
  </div>

  <div class="controlgroup">
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>

  <div class="controlgroup">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div class="controlgroup">
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>

  <div class="controlgroup">
    <%= f.label :zipcode %>
    <%= f.text_field :zipcode %>
  </div>

  <div class="controlgroup">
    <%= f.submit "Save" %>
  </div>
<% end %>
